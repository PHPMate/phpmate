includes:
    - vendor/phpstan/phpstan-phpunit/extension.neon
    - vendor/phpstan/phpstan-phpunit/rules.neon

parameters:
    level: max
    paths:
        - src
        - bin
        - tests

    earlyTerminatingMethodCalls:
        PHPMate\Dashboard\Security\BasicAuthChecker:
            - terminateWithAuthenticationRequest
