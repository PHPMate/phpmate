<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="PHPMate\Domain\Process\JobProcess">
        <id name="job" association-key="true" />
        <id name="order" type="integer" column="`order`" />

        <embedded name="result" class="PHPMate\Domain\Process\ProcessResult" />

        <many-to-one field="job" target-entity="PHPMate\Domain\Job\Job" inversed-by="processes">
            <join-column name="job_id" referenced-column-name="job_id" />
        </many-to-one>
    </entity>
</doctrine-mapping>
