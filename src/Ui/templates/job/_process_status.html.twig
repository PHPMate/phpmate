<span class="process-status" id="process-status-{{ processId }}">
    {% if succeeded %}
        <i class="text-success far fa-check-circle"></i>
    {% elseif failed %}
        <i class="text-danger far fa-times-circle"></i>
    {% else %}
        <i class="text-warning fas fa-circle-notch"></i>
    {% endif %}
</span>
